{
    "name": "cshan/widget-corp-oops",
    "description": "Composer setup for Widget Corp OOPs project",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Widget_Corps_Oops\\": "/",
            "Widget_Corps_Oops_Admin\\": "admin/",
            "Widget_Corps_Oops_Auth\\": "auth/",
            "Widget_Corps_Oops_Helper\\": "helper/",
            "Widget_Corps_Oops_Includes\\": "includes/",
            "Widget_Corps_Oops_Frontend\\": "frontend/"
        }
    },
    "authors": [
        {
            "name": "Shantanoo Chandorkar"
        }
    ],
    "require-dev": {
        "szepeviktor/phpstan-wordpress": "^2.0",
        "squizlabs/php_codesniffer": "^3.13",
        "wp-coding-standards/wpcs": "^3.2",
        "php-stubs/wordpress-globals": "^0.2.0",
        "php-stubs/wordpress-stubs": "^6.8",
        "php-stubs/wp-cli-stubs": "^2.12"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}